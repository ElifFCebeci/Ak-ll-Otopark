<?xml version="1.0" encoding="UTF-8"?>
<protocol>
    <message>
        <header pattern="^DEV:\d+;" />
        <body>
            <field name="TEMP" pattern="TEMP:\d+(\.\d+)?;" />
            <field name="HUM" pattern="HUM:\d+(\.\d+)?;" />
            <field name="FLOOR" pattern="FLOOR:\d+;" />
            <field name="SLOT" pattern="SLOT:\d+" />
        </body>
        <footer pattern="#" />
    </message>
</protocol>
